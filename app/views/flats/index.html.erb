<h1>My Flats</h1>

<ul id="flats">
  <% @flats.each do |flat| %>
    <li>
    <h3>
      <%= link_to flat.name, flat_path(flat) %>.
      <small>
      <%= link_to flat_path(flat.id), method: :delete, data: { confirm: "Delete #{flat.name}?" } do %>
        <i class="fa fa-trash-o" aria-hidden="true"></i>
      <% end %>
      </small>
    </h3>
      <p><%= flat.description %></p>
    </li>
  <% end %>
</ul>
<i class="large material-icons">account_balance</i>


